{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "src",
    "paths": {
      "@lib/*": ["lib/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@assets/*": ["assets/*"],
      "@pages/*": ["content/pages/*"],
      "@content/*": ["content/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"]
    },
    "types": ["unplugin-icons/types/astro"]
  }
}
